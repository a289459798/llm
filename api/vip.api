syntax = "v1"

type VipPriceResponse {
    Original int `json:"original"`
    Price int `json:"price"`
}

type VipGiveResponse {
    Day int `json:"day"`
    Expiry string `json:"expiry"`
}

@server(
    jwt: Auth
    middleware: Sign
    group: vip
)
service tools {
    @handler vipPrice
    get /vip/price returns (VipPriceResponse)

    @handler vipGive
    get /vip/give returns (VipGiveResponse)
}