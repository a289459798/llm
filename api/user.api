syntax = "v1"

type LoginRequest {
}

type LoginResponse {
}

type InfoRequest {
}

type InfoResponse {
}

@server(
    group: user
)
service tools {
    @handler login
    get /users/login (LoginRequest) returns (LoginResponse)
}

@server(
    middleware: Auth
    group: user
)
service tools {
    @handler userInfo
    get /users/:id (InfoRequest) returns (InfoResponse)
}